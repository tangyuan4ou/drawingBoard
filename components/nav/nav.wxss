@import './iconfont.wxss';

/**nav.wxss**/
.nav_container {
  position: absolute;
  bottom: 0; left: 0;
  width: 100%;
  height: 100rpx;
  padding-left: 50rpx;
}

.nav_block {
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}

.nav_icon {
  display: inline-block;
  width: 100%; height: 100%;
  font-size: 50rpx;
  text-align: center;
  line-height: 100rpx;
}

.icon_block_img {
  display: inline-block;
  vertical-align: middle;
  width: 60rpx; height: 60rpx;
  padding-bottom: 20rpx;
}